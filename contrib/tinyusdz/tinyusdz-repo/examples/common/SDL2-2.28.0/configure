CheckDLOPEN()
{

printf "%s\n" "#define DYNAPI_NEEDS_DLOPEN 1" >>confdefs.h


    ac_fn_c_check_header_compile "$LINENO" "dlfcn.h" "ac_cv_header_dlfcn_h" "$ac_includes_default"
if test "x$ac_cv_header_dlfcn_h" = xyes
