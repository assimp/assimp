            fi

            # Check to see if pthread naming is available
            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for pthread_setname_np" >&5
printf %s "checking for pthread_setname_np... " >&6; }
            cat confdefs.h - <<_ACEOF >conftest.$ac_ext
