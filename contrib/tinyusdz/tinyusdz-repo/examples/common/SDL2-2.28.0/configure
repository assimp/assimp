if test $ac_have_decl = 1
then :
  libdecor_get_min_max=yes
fi

                    if test x$libdecor_get_min_max = xyes; then

printf "%s\n" "#define SDL_HAVE_LIBDECOR_GET_MIN_MAX 1" >>confdefs.h

                    fi
                    CFLAGS="$saved_cflags"
                fi
            fi
        fi
    fi
