#usda 1.0

def "bora" (
    append variantSets = ["shapeVariant"]
    variants = {
      string shapeVariant = "Capsule"
    }
)
{
    variantSet "shapeVariant" = {
        "Capsule" ( hidden = 0 ) {
            def "bora" {
               float mycapsule = 1.3
            }
        }
        "Cone" ( hidden = 1 ) {
            def "bora" {
               float mycone = 2.3
            }
        }
    }

}
